<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jbp.service.dao.UserVisaOrderDao">


    <select id="getAdminPageList" resultType="com.jbp.common.response.UserVisaOrderRecordResponse">

		select  u.nickName,e.* from eb_user_visa_order e
		left join eb_user u on u.id  = e.uid
		where 1=1
		<if test="account !=null">
			and u.account=#{account}
		</if>


	</select>
</mapper>
